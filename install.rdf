<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>{cad52e60-d607-11e2-8b8b-0800200c9a66}</em:id>
    <em:name>Google Music Media Key Fix</em:name>
    <em:description>A fix to enable global use of media keys in Google Play Music. Once installed, this extension will allow you to control an open Google Play Music tab from anywhere in Windows using the play/pause, stop, next, and prev keys that some keyboards provide. No configuration necessary.</em:description>
    <em:version>1.1</em:version>
    <em:creator></em:creator>
    <em:type>2</em:type>
    <em:unpack>true</em:unpack>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>10.0</em:minVersion>
        <em:maxVersion>22.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Windows -->
    <em:targetPlatform>WINNT</em:targetPlatform>
  </Description>
</RDF>
